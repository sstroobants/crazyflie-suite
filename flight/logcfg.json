{
    "twr":{
        "name": "twr",
        "type": "CF",
        "period": 10,
        "variables": ["ranging.distance0",
                      "ranging.distance1",
                      "ranging.distance2",
                      "ranging.distance3"],
        "headers": ["twr0", "twr1", "twr2", "twr3"]
    },

    "tdoa":{
        "name": "tdoa",
        "type": "CF",
        "period": 10,
        "variables": ["tdoa.diff01",
                      "tdoa.diff02",
                      "tdoa.diff03",
                      "tdoa.diff12",
                      "tdoa.diff13",
                      "tdoa.diff23"],
        "headers": ["tdoa01", "tdoa02", "tdoa03", "tdoa12", "tdoa13", "tdoa23"]
    },

    "gyros":{
        "name": "gyros",
        "type": "CF",
        "period": 10,
        "variables": ["gyro.x",
                      "gyro.y",
                      "gyro.z"],
        "headers": ["gyroX", "gyroY", "gyroZ"]
    },

    "attitude":{
        "name": "attitude",
        "type": "CF",
        "period": 10,
        "variables": ["stabilizer.roll",
                      "stabilizer.pitch",
                      "stabilizer.yaw"],
        "headers": ["roll", "pitch", "yaw"]
    },

    "acc":{
        "name": "acc",
        "type": "CF",
        "period": 10,
        "variables": ["acc.x",
                      "acc.y",
                      "acc.z"],
        "headers": ["accX", "accY", "accZ"]
    },

    "kalman":{
        "name": "kalman",
        "type": "CF",
        "period": 10,
        "variables": ["kalman.stateX",
                      "kalman.stateY",
                      "kalman.stateZ"],
        "headers": ["kalX", "kalY", "kalZ"]
    },
    
    "laser":{
        "name": "laser",
        "type": "CF",
        "period": 10,
        "variables": ["range.zrange"],
        "headers": ["laserHeight"]
    },
    
    "flow":{
        "name": "flow",
        "type": "CF",
        "period": 10,
        "variables": ["motion.deltaX",
                      "motion.deltaY"],
        "headers": ["flowX", "flowY"]
    },

    "state":{
        "name": "state",
        "type": "CF",
        "period": 10,
        "variables": ["stateEstimate.x",
                      "stateEstimate.y",
                      "stateEstimate.z"],
        "headers": ["stateX", "stateY", "stateZ"]
    },

    "otpos":{
        "name": "otpos",
        "type": "EXT",
        "period": 10,
        "variables": ["otX",
                      "otY",
                      "otZ"],
        "headers": ["otX", "otY", "otZ"]
    },
    
    "otatt":{
        "name": "otatt",
        "type": "EXT",
        "period": 10,
        "variables": ["otRoll",
                      "otPitch",
                      "otYaw"],
        "headers": ["otRoll", "otPitch", "otYaw"]
    }
}
