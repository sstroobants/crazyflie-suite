{
    "gyros":{
        "name": "gyros",
        "type": "CF",
        "period": 10,
        "variables": ["gyro.x",
                      "gyro.y",
                      "gyro.z"],
        "headers": ["gyroX", "gyroY", "gyroZ"]
    },

    "attitude":{
        "name": "attitude",
        "type": "CF",
        "period": 10,
        "variables": ["stabilizer.roll",
                      "stabilizer.pitch",
                      "stabilizer.yaw"],
        "headers": ["roll", "pitch", "yaw"]
    },

    "kalman":{
        "name": "kalman",
        "type": "CF",
        "period": 10,
        "variables": ["kalman.stateX",
                      "kalman.stateY",
                      "kalman.stateZ"],
        "headers": ["kalX", "kalY", "kalZ"]
    },    

    "acc":{
        "name": "acc",
        "type": "CF",
        "period": 10,
        "variables": ["acc.x",
                      "acc.y",
                      "acc.z"],
        "headers": ["accX", "accY", "accZ"]
    },

    "state":{
        "name": "state",
        "type": "CF",
        "period": 10,
        "variables": ["stateEstimate.x",
                      "stateEstimate.y",
